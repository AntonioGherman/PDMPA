<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Label">
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">#9E9E9E</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>

    <style name="Input">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@drawable/bg_input_scan</item>
        <item name="android:padding">10dp</item>
        <item name="android:textSize">13sp</item>
    </style>


    <style name="ButtonPrimary">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:background">@drawable/bg_round_black</item>
    </style>

    <style name="ButtonOutline">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:background">@drawable/bg_square_outline</item>
    </style>


</resources>